{
	"name": "@whoplabs/whop-client",
	"version": "0.5.3",
	"description": "Whop account SDK",
	"homepage": "https://github.com/wheblabs/whop-client#readme",
	"bugs": {
		"url": "https://github.com/wheblabs/whop-client/issues"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/wheblabs/whop-client.git"
	},
	"scripts": {
		"build": "bunup && bun run build:fix-duplicate",
		"build:fix-duplicate": "node -e \"const fs=require('fs');const f='dist/shared/chunk-7jxqf0jv.js';let c=fs.readFileSync(f,'utf8');c=c.replace(/^export {\\s*\\n\\s*graphqlRequest\\s*\\n};\\s*\\n/gm,'');fs.writeFileSync(f,c);\"",
		"dev": "bunup --watch",
		"prepare": "bun simple-git-hooks",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"release": "bumpp --commit --push --tag",
		"test": "bun test",
		"test:coverage": "bun test --coverage",
		"test:watch": "bun test --watch",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.6",
		"@types/bun": "^1.3.0",
		"bumpp": "^10.3.1",
		"bunup": "^0.15.3",
		"simple-git-hooks": "^2.13.1",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"module": "./dist/index.js",
	"simple-git-hooks": {
		"pre-commit": "bun run lint && bun run type-check"
	},
	"types": "./dist/index.d.ts"
}
